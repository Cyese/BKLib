<div class="mt-4">
    <div>
      <h3>Danh sach tai khoan</h3>
    </div>

    <table class="table mt-4">
        <thead>
            <tr>
                <th scope="col">No</th>
                <th scope="col">User name</th>
                <th scope="col">Email</th>
            </tr>
        </thead>
        <tbody>
            {{#each accounts}}
            <tr>
                <th scope="row">{{sum @index 1}}</th>
                <td>{{this.name}}</td>
                <td>{{this.email}}</td>
            </tr>
            {{else}}
            <tr>
              <td colspan="5" class="text-center">
                Chua co tai khoan nao 
              </td>
            </tr>
            {{/each}}
        </tbody>
    </table>
</div>